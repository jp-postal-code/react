{
  "name": "@jp-postal-code/react",
  "description": "JP Postal Code API for React",
  "version": "0.1.2",
  "repository": "git@github.com:jp-postal-code/react.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module",
  "author": "shota <xyz.shotao@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "swc scripts -d scripts && node ./scripts/prebuild.js",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "node ./scripts/postbuild.js",
    "test": "jest",
    "start-examples:next": "cd examples/next && yarn && yarn dev",
    "e2e": "start-server-and-test start-examples:next http://localhost:3000 'cypress run'",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "default": "./index.js"
    }
  },
  "sideEffects": false,
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@jest/globals": "29.3.1",
    "@jp-postal-code/api-client": "0.1.2",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.21",
    "@swc/jest": "0.2.23",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "cypress": "12.5.1",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "start-server-and-test": "1.14.0",
    "typescript": "4.9.3",
    "whatwg-fetch": "3.6.2"
  },
  "peerDependencies": {
    "@jp-postal-code/api-client": "^0.1.0",
    "react": "^15.0.0 | ^16.0.0 | ^17.0.0 | ^18.0.0"
  }
}
